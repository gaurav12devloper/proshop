  "bugs": {
    "url": "https://github.com/gaurav12devloper/proshop/issues"
  },
  "homepage": "https://github.com/gaurav12devloper/proshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "vconcol": "^1.0.1"
  },
